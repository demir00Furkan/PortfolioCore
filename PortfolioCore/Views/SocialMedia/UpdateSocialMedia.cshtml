@model SocialMedia
@{
    ViewData["Title"] = "UpdateSocialMedia";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h1>Sosyal Medya Güncelleme</h1>
<form method="post">
    <input type="hidden" asp-for="SocialMediaId" />
    <label>Başlık</label>
    <input type="text" asp-for="SocialMediaTitle" class="form-control" />
    <br />
    <label>Url</label>
    <input type="text" asp-for="SocialMediaUrl" class="form-control" />
    <br />
    <label>Icon</label>
    <input type="text" asp-for="SocialMediaIcon" class="form-control" />
    <br />
    <button class="btn btn-primary">Değişiklikleri Kaydet</button>



</form>


