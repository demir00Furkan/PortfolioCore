@model SocialMedia

@{
    ViewData["Title"] = "CreateSocialMedia";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h1>Yeni Proje Girişi</h1>
<form method="post">
    <label>Başlık</label>
    <input type="text" asp-for="SocialMediaTitle" class="form-control" />
    <br />
    <label>Url</label>
    <input type="text" asp-for="SocialMediaUrl" class="form-control" />
    <br />
    <label>Icon</label>
    <input type="text" asp-for="SocialMediaIcon" class="form-control" />
    <br />
    <button class="btn btn-primary">Kaydet</button>



</form>

